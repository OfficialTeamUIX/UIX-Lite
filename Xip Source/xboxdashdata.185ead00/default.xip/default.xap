function GetSavedValue(x,y) 
{
	var a = x;
	var b = y;
	ConfigFile.section = a;
	var c = ConfigFile.GetValue(b);
	return c;
}

function SetSavedValue(x,y,z) 
{
	var a = x;
	var b = y;
	var c = z;
	ConfigFile.section = a;
	ConfigFile.SetValue(b,c);
}

function GetSavedBooleanValue(x,y) 
{
	var a = x;
	var b = y;
	ConfigFile.section = a;
	var c = ConfigFile.GetValue(b);
	if(c.toLowerCase() == "true")
	{
		return true;
	}
	else
	{
		return false;
	}
}

function SetSavedBooleanValue(x,y,z) 
{
	var a = x;
	var b = y;
	var c = z;
	ConfigFile.section = a;
	if(c)
	{
		ConfigFile.SetValue(b,"true");
	}
	else
	{
		ConfigFile.SetValue(b,"false");
	}
}
